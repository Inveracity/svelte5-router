{
  "name": "@mateothegreat/svelte5-router",
  "version": "0.1.21",
  "type": "module",
  "svelte": "./src/lib/index.ts",
  "exports": {
    ".": {
      "svelte": "./src/lib/index.ts",
      "types": "./src/lib/types.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "check": "svelte-check --tsconfig ./tsconfig.json",
