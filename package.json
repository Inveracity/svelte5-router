{
  "name": "@mateothegreat/svelte5-router",
  "version": "2.16.15",
  "type": "module",
  "moduleResolution": "bundler",
  "sideEffects": false,
  "license": "MIT",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "svelte": "./index.js"
    }
  },
  "files": [
    "./**/*"
  ],
  "readme": "README.md",
  "description": "ðŸ«¦ An SPA router for Svelte that allows you to divide & conquer your app with nested routers, snippets, and more.",
  "homepage": "https://github.com/mateothegreat/svelte5-router",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mateothegreat/svelte5-router.git"
  },
  "author": {
    "name": "Matthew Davis",
    "email": "matthew@matthewdavis.io",
    "url": "https://matthewdavis.io"
  },
  "keywords": [
    "router",
    "spa router",
    "svelte",
    "svelte router",
    "svelte spa router",
    "svelte 5",
    "svelte 5 router",
    "svelte 5 spa router"
  ],
  "scripts": {
    "check": "svelte-check",
    "build": "npm run check && rm -rf dist && npx svelte-package && cp docs/assets/logo.png dist && cp docs/readme.md dist/README.md",
    "build:esm+cjs": "npm run check && rm -rf dist && npx vite build && cp package.json docs/assets/logo.png dist && cp docs/readme.md dist/README.md",
    "docs:build": "typedoc --footerLastModified --customTitle \"svelte 5 router\" ",
    "docs:watch": "typedoc --watch --footerLastModified --customTitle \"svelte 5 router\" ",
    "docs:serve": "cd tmp/build && npx httpserver -p 20000",
    "test:ci": "npx vitest run --coverage",
    "test:watch": "npx vitest"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@sveltejs/package": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@svitejs/changesets-changelog-github-compact": "^1.2.0",
    "@tsconfig/svelte": "^5.0.5",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.38.6",
    "svelte-check": "^4.3.1",
    "svelte-preprocess": "^6.0.3",
    "typescript": "^5.9.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  }
}
