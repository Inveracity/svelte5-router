// https://app.studyraid.com/en/courses/15016/typedoc-automated-api-documentation-for-typescript
{
  "$schema": "https://typedoc.org/schema.json",
  "blockTags": [
    "@defaultValue",
    "@deprecated",
    "@example",
    "@param",
    "@privateRemarks",
    "@remarks",
    "@returns",
    "@see",
    "@throws",
    "@typeParam",
    "@author",
    "@callback",
    "@category",
    "@categoryDescription",
    "@default",
    "@document",
    "@extends",
    "@augments",
    "@yields",
    "@group",
    "@groupDescription",
    "@import",
    "@inheritDoc",
    "@jsx",
    "@license",
    "@module",
    "@mergeModuleWith",
    "@prop",
    "@property",
    "@return",
    "@satisfies",
    "@since",
    "@template",
    "@type",
    "@typedef",
    "@summary",
    "@optional",
    "@source",
    "@usage",
    "@note",
    "@todo"
  ],
  "categorizeByGroup": false,
  "categoryOrder": ["actions", "components", "hooks", "utilities"],
  "cleanOutputDir": true,
  "commentStyle": "all",
  "copyAssets": {
    "copyHtmlImgTag": true,
    "exclude": [],
    "excludePath": [".*?exclude/.*?$"],
    "include": [],
    "includePath": [".(png|md)$"],
    "onlyImages": true
  },
  "coverageLabel": "Documentation Coverage",
  "coverageOutputPath": "docs/assets/coverage.svg",
  "coverageOutputType": "all",
  "coverageSvgWidth": 190,
  "customValidation": {
    "byKind": [
      {
        "kinds": "Function",
        "summary": true,
        "tags": ["example"]
      }
    ]
  },
  "entryPoints": ["../src/lib/index.ts"],
  "exclude": ["**/*+(.spec|.e2e).ts", "**/*.svg"],
  "excludeExternals": true,
  "excludeInternal": true,
  "excludeNotDocumented": false,
  "externalPattern": ["**/*.md"],
  "githubPages": false,
  "groupReferencesByType": false,
  "headings": {
    "document": false,
    "readme": false
  },
  "hideGenerator": true,
  "highlightLanguages": ["typescript", "javascript", "svelte", "bash", "shell", "shellscript"],
  "includeVersion": true,
  "modifierTags": ["@alpha", "@beta", "@experimental"],
  "navigation": {
    "includeCategories": true,
    "compactFolders": true
  },
  "navigationLinks": {
    "Demo": "https://demo.router.svelte.spa",
    "GitHub": "https://github.com/mateothegreat/svelte5-router",
    "NPM": "https://www.npmjs.com/package/@mateothegreat/svelte5-router",
    "Releases": "https://github.com/mateothegreat/svelte5-router/releases",
    "Report Issue": "https://github.com/mateothegreat/svelte5-router/issues"
  },
  "out": "../tmp/build",
  "plugin": [
    "@droppedcode/typedoc-plugin-copy-assets",
    "typedoc-github-theme",
    "typedoc-plugin-inline-sources",
    "typedoc-plugin-extras",
    "typedoc-plugin-include-example",
    "typedoc-plugin-mermaid",
    "typedoc-plugin-coverage",
    "typedoc-plugin-custom-validation",
    "typedoc-plugin-dt-links",
    "@vpalmisano/typedoc-plugin-ga",
    "typedoc-plugin-version-header"
  ],
  "versionHeaderFormat": "{{major}}.{{minor}}.{{patch}}",
  "gaID": "G-FTT7485E5D",
  "readme": "readme.md",
  "searchInComments": true,
  "searchInDocuments": true,
  "sidebarLinks": {
    "Demo": "https://demo.router.svelte.spa",
    "GitHub": "https://github.com/mateothegreat/svelte5-router",
    "Releases": "https://github.com/mateothegreat/svelte5-router/releases",
    "Report Issue": "https://github.com/mateothegreat/svelte5-router/issues"
  },
  "useFirstParagraphOfCommentAsSummary": true,
  "validation": {
    "invalidLink": false,
    "notDocumented": false,
    "notExported": false,
    "rewrittenLink": false,
    "unusedMergeModuleWith": false
  },
  "visibilityFilters": {
    "@alpha": true,
    "@beta": true,
    "external": false,
    "inherited": true,
    "private": true,
    "protected": true
  }
}
