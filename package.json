    "release": "npm run check && npm publish --access public"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.1",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^22.10.1",
    "@vitest/ui": "^2.1.6",
    "cpx": "^1.5.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.1",
    "prettier-plugin-svelte": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "rimraf": "^6.0.1",
    "svelte": "^5.2.11",
    "svelte-check": "^4.1.0",
    "svelte-package": "^0.1.0",
    "tailwind-merge": "^2.5.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.1",
    "vitest": "^2.1.6"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  }
}
